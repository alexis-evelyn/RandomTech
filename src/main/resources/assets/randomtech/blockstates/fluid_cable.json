{
  "multipart": [
    {
      "apply": {
        "model": "randomtech:block/cable/cable"
      }
    },
    {
      "when": {
        "north": "cable|interfaceable"
      },
      "apply": {
        "model": "randomtech:block/cable/cable_side"
      }
    },
    {
      "when": {
        "east": "cable|interfaceable"
      },
      "apply": {
        "model": "randomtech:block/cable/cable_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "cable|interfaceable"
      },
      "apply": {
        "model": "randomtech:block/cable/cable_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "cable|interfaceable"
      },
      "apply": {
        "model": "randomtech:block/cable/cable_side",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "cable|interfaceable"
      },
      "apply": {
        "model": "randomtech:block/cable/cable_side",
        "x": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "down": "cable|interfaceable"
      },
      "apply": {
        "model": "randomtech:block/cable/cable_side",
        "x": 90,
        "uvlock": true
      }
    }
  ]
}